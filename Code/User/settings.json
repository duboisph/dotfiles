{
    "breadcrumbs.enabled": false,
    "editor.fontFamily": "Fira Code",
    "editor.fontLigatures": true,
    "editor.fontSize": 12,
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true,
    "editor.insertSpaces": true,
    "editor.minimap.enabled": false,
    "editor.multiCursorModifier": "alt",
    "editor.renderWhitespace": "boundary",
    "editor.rulers": [
        80
    ],
    "editor.tabSize": 2,
    "editor.wordWrap": "on",
    "editor.wordWrapColumn": 80,
    "explorer.confirmDragAndDrop": false,
    "files.eol": "\n",
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "git.autofetch": false,
    "git.autorefresh": false,
    "gitlens.advanced.telemetry.enabled": false,
    "gitlens.blame.compact": true,
    "gitlens.codeLens.scopes": [
        "document"
    ],
    "markdownlint.config": {
        "MD013": false,
        "MD024": {
            "siblings_only": true
        }
    },
    "telemetry.enableTelemetry": false,
    "terminal.integrated.shell.windows": "C:\\WINDOWS\\System32\\wsl.exe",
    "terraform.lintPath": "C:\\Users\\hello\\bin\\tflint.exe",
    "terraform.path": "C:\\Users\\hello\\bin\\terraform.exe",
    "vs-kubernetes": {
        "vs-kubernetes.helm-path": "C:\\Users\\hello\\.vs-kubernetes\\tools\\helm\\windows-amd64\\helm.exe",
        "vs-kubernetes.minikube-path": "C:\\Users\\hello\\.vs-kubernetes\\tools\\minikube\\windows-amd64\\minikube.exe",
        "vs-kubernetes.draft-path": "C:\\Users\\hello\\.vs-kubernetes\\tools\\draft\\windows-amd64\\draft.exe"
    },
    "window.zoomLevel": 0,
    "workbench.colorTheme": "Dracula Soft",
    "workbench.iconTheme": "material-icon-theme"
}
